  "tc 2.1 Create a role skill with two skills": {
    "headers": "",
    "create-role": "mutation{RoleCreateOne(name: \"Full Stack Developer\"){id}}",
    "skills":{
      "create-skill-one": "mutation{SkillCreateOne(name: \"Typescript\"){id}}",
      "create-skill-two": "mutation{SkillCreateOne(name: \"Java\"){id}}"
    },
    "payload": "mutation($role_id: Int!, $skill_id1: Int!, $skill_id2: Int!){RoleSkillsOverwrite(roleId: $role_id, skills:[{skillId: $skill_id1, weight: 0.6}, {skillId:$skill_id2, weight: 0.4}]){roleId, id, roleId, skill{id, name}}}",
    "expected-status-code": 200,
    "expected-response": {
      "data": {
        "RoleSkillsOverwrite": [
          {
            "id": "::FUNC::validate_id",
